<form action="/route">
    <div class="form-row">

        <div class="form-group">
            <label for="inputStart">Start Location</label>
            <input type="text" class="form-control" id="inputStart" placeholder="Road, City, State">
        </div>

        <div class="form-group">
            <label for="inputEnd">End Location</label>
            <input type="text" class="form-control" id="inputEnd" placeholder="Road, City State">
        </div>

        <div>
            <button type="submit" class="btn btn-primary" id="searchBtn">Sign in</button>
        </div>
    </div>
</form>

<script src="/assets/js/script.js"></script>